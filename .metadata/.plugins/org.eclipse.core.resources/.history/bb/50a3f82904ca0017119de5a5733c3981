package com.example.maven.calclulator_susanne;

import static org.junit.Assert.*;

import org.junit.Test;

public class CalculatorTest {
	Calculator calc = new Calculator();

	@Test
	public void testAdd() {
		assertEquals(calc.add(2, 2), 4,0);
		double value = calc.add(2, 2);
		assertTrue(value == 4);
	}

}
