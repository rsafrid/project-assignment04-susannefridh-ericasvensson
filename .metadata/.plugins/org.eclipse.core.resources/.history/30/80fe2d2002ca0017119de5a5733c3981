package com.example.maven.calclulator_susanne;

public class Main {

	public static void main(String[] args) {
		Calculator calc = new Calculator();
		System.out.println(calc.add(2,  2));
		System.out.println(calc.divide(2, 2));
		System.out.println(calc.subtract(2, 4));
		System.out.println(calc.multiply(3, 3));

	}

}
